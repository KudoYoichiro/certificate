<h1>Listing service centers</h1>

<table class="table table-bordered">
  <thead>
    <tr class="certificate_table_caption">
      <th>Service Center</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @service_centers.each do |service_center| %>
      <tr>
        <td><%= service_center.name %></td>
        <td>
        	<%= link_to 'Show', service_center, class: :crud_control %>
        	<%= link_to 'Edit', edit_service_center_path(service_center), class: :crud_control %>
        	<%= link_to 'Destroy', service_center, method: :delete, data: { confirm: 'Are you sure?' }, class: :crud_control %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Service center', new_service_center_path %>
